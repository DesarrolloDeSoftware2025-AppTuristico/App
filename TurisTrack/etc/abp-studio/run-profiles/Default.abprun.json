{
  "applications": {
    "TurisTrack.HttpApi.Host": {
      "type": "dotnet-project",
      "launchUrl": "https://localhost:44340",
      "path": "../../../src/TurisTrack.HttpApi.Host/TurisTrack.HttpApi.Host.csproj",
      "kubernetesService": ".*-httpapihost$",
      "healthCheckEndpoint": "/health-status",
      "healthUiEndpoint": "/health-ui",
      "execution": {
        "order": 4
      }
    },
    "TurisTrack.Angular": {
      "type": "cli",
      "workingDirectory": "../../../angular",
      "startCommand": "./start.ps1",
      "launchUrl": "http://localhost:4200",
      "kubernetesService": ".*-angular$",
      "execution": {
        "order": 2
      }    
    }
  },
  "containers": {
    "serviceName": "TurisTrack-Containers",
  }
}